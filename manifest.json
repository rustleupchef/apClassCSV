{
    "manifest_version": 2,
    "name": "AP Class to Blooket",
    "version": "1.1",
    "description": "Is able to convert ap class room webpages to a csv for blooket",
    "permissions": ["activeTab", "scripting"],
    "browser_action": {
        "default_popup": "popup/popup.html",
        "default_icon": "icons/AP.png"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js" : ["content.js"],
            "run_at": "document_end"
        }
    ]
}
